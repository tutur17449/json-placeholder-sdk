{
  "env": {
    "jest": true,
    "node": true
  },
  "parser": "@typescript-eslint/parser",
  "extends": "eslint:recommended",
  "settings": {
    "import/extensions": [".js", ".jsx", ".ts", ".tsx"],
    "import/parsers": {
      "@typescript-eslint/parser": [".ts", ".tsx"]
    }
  },
  "rules": {
    "max-len": ["error", { "code": 80 }],
    "implicit-arrow-linebreak": 0,
    "no-console": 0,
    "camelcase": 0,
    "no-undef": "warn",
    "object-curly-newline": "off"
  }
}
